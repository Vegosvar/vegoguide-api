{
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["jest"],
  "env": {
    "node": true,
    "jest/globals": true
  },
  "rules": {
    "quotes": [2, "single"],
    "no-underscore-dangle": 0
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
  }
}
